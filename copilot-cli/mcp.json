{
  "mcpServers": {
    "github-mcp-server": {
      "type": "http",
      "url": "https://api.githubcopilot.com/mcp",
      "tools": ["*"],
      "headers": {
        "X-MCP-Toolsets": "repos,issues,users,pull_requests,code_security,secret_protection,actions,web_search"
      }
    },
    "sequential-thinking": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "sequential-thinking-slim@latest"],
      "tools": ["*"]
    },
    "serena": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "serena-slim@latest"],
      "tools": ["*"]
    },
    "context7": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "context7-slim@latest"],
      "tools": ["*"]
    },
    "read-website": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@just-every/mcp-read-website-fast"],
      "tools": ["*"]
    },
    "think-tool": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "think-tool-mcp"],
      "tools": ["*"]
    },
    "exa": {
      "type": "http",
      "url": "https://mcp.exa.ai/mcp?tools=web_search_exa,web_search_advanced_exa,get_code_context_exa,crawling_exa,deep_researcher_start,deep_researcher_check",
      "tools": ["*"]
    },
    "fast-filesystem-mcp": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "fast-filesystem-mcp"],
      "tools": ["*"]
    },
    "code-mode-toon": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "code-mode-toon"],
      "tools": ["*"]
    },
    "praetorian-opt": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "claude-praetorian-mcp"],
      "tools": ["*"]
    }    
  }
}
